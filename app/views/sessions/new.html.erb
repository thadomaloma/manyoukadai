<div class="card">
  <div class="card-body">
    <h3>Login</h3>
    	<%= form_with(scope: :session, url: sessions_path, local:true) do |f| %>
    	  <%= f.label :email %>
    	  <%= f.email_field :email %>
    	  <%= f.label :password %>
    	  <%= f.password_field :password %>
    	  <%= f.submit "login" %>
    	<% end %>
  </div>
</div>
